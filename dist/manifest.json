{
	"manifest_version": 3,
	"name": "LinkMD",
	"description": "Create a markdown link of the current page with quoted text.",
	"version": "1.0",
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"64": "icon-64.png",
			"128": "icon-128.png"
		}
	},
	"icons": {
		"64": "icon-64.png",
		"128": "icon-128.png"
	},
	"permissions": [
		"activeTab",
		"tabs",
		"scripting"
	]
}